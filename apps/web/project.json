{
  "name": "web",
  "sourceRoot": "apps/web/src",
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd apps/web && npx vite"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd apps/web && npx vite build"
      },
      "dependsOn": ["shared:build", "api-client:build"]
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd apps/web && npx eslint src/"
      }
    }
  }
}
